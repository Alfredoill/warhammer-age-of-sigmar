    <rule id="42cf-2dd1-7eb8-933e" name="Fire on the Move" hidden="false" book="WEW" page="13">
      <description>Can run and shoot in the same turn.</description>
      <modifiers/>
    </rule>
    <rule id="664c-a770-4953-367d" name="Musician" hidden="false" book="">
