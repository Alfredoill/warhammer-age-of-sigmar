    <entry id="340b-a35b-913f-aa3f" name="Slann Mage-Priest" points="7.0" categoryId="b6e2-4831-e39f-d052" type="model" minSelections="0" maxSelections="-1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" book="Lizardmen Warscroll" page="4">
